{
  "name": "devprompt-studio",
  "version": "2.1.0",
  "type": "module",
  "description": "Git for prompts. Version-controlled prompt engineering workspace for developers with P2P collaboration.",
  "keywords": [
    "prompt-engineering",
    "llm-ops",
    "ai-tools",
    "developer-tools",
    "prompt-testing",
    "version-control",
    "team-collaboration",
    "p2p",
    "webrtc",
    "real-time-collaboration"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/CJCreator-New/dev_promptstudio"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build",
    "test": "vitest",
    "test:unit": "vitest run src/test-strategy/unit",
    "test:integration": "vitest run src/test-strategy/integration",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:visual": "playwright test -c playwright-visual.config.ts",
    "test:perf": "vitest run src/test-strategy/performance",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project=mobile-chrome --project=mobile-safari --project=iphone-14 --project=galaxy-s23",
    "test:e2e:edge": "playwright test --project=edge",
    "test:e2e:tablet": "playwright test --project=ipad",
    "test:e2e:critical": "playwright test e2e/critical",
    "test:e2e:auth": "playwright test e2e/critical/auth-stability.spec.ts",
    "test:e2e:auth-only": "playwright test e2e/auth.spec.ts",
    "test:e2e:no-auth": "playwright test --grep-invert 'auth|login'",
    "test:e2e:fast": "playwright test --config=playwright-fast.config.ts",
    "test:e2e:report": "playwright show-report",
    "perf:lighthouse": "lhci autorun",
    "perf:analyze": "vite-bundle-visualizer",
    "a11y:test": "playwright test --project=accessibility",
    "a11y:test:chromium": "playwright test e2e/accessibility --project=chromium",
    "a11y:audit": "node scripts/a11y-audit.js",
    "a11y:report": "node scripts/a11y-report.js",
    "lint": "tsc --noEmit",
    "lint:a11y": "eslint --ext .tsx,.ts src --plugin jsx-a11y",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@types/canvas-confetti": "^1.9.0",
    "@types/crypto-js": "^4.2.2",
    "canvas-confetti": "^1.9.4",
    "crypto-js": "^4.2.0",
    "dexie": "^3.2.4",
    "diff-match-patch": "^1.0.5",
    "dompurify": "^3.3.0",
    "file-saver": "^2.0.5",
    "firebase": "^12.6.0",
    "framer-motion": "^10.16.4",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "jszip": "^3.10.1",
    "lucide-react": "^0.263.1",
    "lz-string": "^1.5.0",
    "monaco-editor": "^0.55.1",
    "peerjs": "^1.5.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-swipeable": "^7.0.1",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.10",
    "reactflow": "^11.11.4",
    "web-vitals": "^3.5.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.3",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/crypto-js": "^4.2.1",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^22.14.0",
    "@types/peerjs": "^0.0.30",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "@vitest/ui": "^3.0.0",
    "autoprefixer": "^10.4.22",
    "axe-core": "^4.8.3",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "fast-check": "^3.15.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^23.0.1",
    "pa11y": "^8.0.0",
    "pa11y-ci": "^4.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "terser": "^5.26.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^3.0.0"
  }
}
